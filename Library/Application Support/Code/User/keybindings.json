[{
    "key": "cmd+enter",
    "command": "renameFile",
    "when": "explorerViewletVisible && filesExplorerFocus"
  }, {
    "key": "enter",
    "command": "-renameFile",
    "when": "explorerViewletVisible && filesExplorerFocus"
  },
  {
    "key": "enter",
    "command": "list.select",
    "when": "explorerViewletVisible && filesExplorerFocus"
  },
  {
    "key": "shift+cmd+x",
    "command": "editor.action.deleteLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+k",
    "command": "expand_region",
    "when": "editorTextFocus"
  }, {
    "key": "alt+j",
    "command": "undo_expand_region",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+ctrl+'",
    "command": "extension.selectEitherQuote"
  }, {
    "key": "shift+alt+ctrl+0",
    "command": "extension.selectParenthesis"
  }, {
    "key": "shift+alt+ctrl+9",
    "command": "extension.selectParenthesis"
  }, {
    "key": "shift+alt+ctrl+`",
    "command": "extension.selectBackTick"
  }, {
    "key": "shift+alt+ctrl+[",
    "command": "extension.selectCurlyBrackets"
  }, {
    "key": "shift+alt+ctrl+]",
    "command": "extension.selectCurlyBrackets"
  },
  {
    "key": "cmd+o",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "cmd+p",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "cmd+down",
    "command": "-editor.action.goToDeclaration",
    "when": "editorHasDefinitionProvider && editorTextFocus"
  },
  {
    "key": "alt+up",
    "command": "-editor.action.smartSelect.grow",
    "when": "editorTextFocus"
  },
    {
    "key": "alt+down",
    "command": "-editor.action.smartSelect.shrink",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+;",
    "command": "-workbench.action.tasks.configureTaskRunner"
  },
  {
    "key": "cmd+;",
    "command": "breadcrumbs.focus",
    "when": "breadcrumbsVisible"
  },
  {
    "key": "shift+cmd+;",
    "command": "-breadcrumbs.focus",
    "when": "breadcrumbsVisible"
  },
  {
    "key": "ctrl+tab",
    "command": "workbench.action.openPreviousEditorFromHistory"
  }, {
    "key": "ctrl+tab",
    "command": "workbench.action.quickOpenNavigateNext",
    "when": "inQuickOpen"
  },
  {
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "cmd+1",
    "command": "workbench.view.explorer",
    "when": "editorFocus"
  }, {
    "key": "cmd+1",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "!editorFocus"
  },
  {
    "key": "cmd+5",
    "command": "workbench.view.debug",
    "when": "editorFocus"
  }, {
    "key": "cmd+5",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "!editorFocus"
  },
  {
    "key": "cmd+9",
    "command": "workbench.view.scm",
    "when": "editorFocus"
  }, {
    "key": "cmd+9",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "!editorFocus"
  },
  {
    "key": "cmd+shift+f",
    "command": "workbench.action.findInFiles",
    "when": "editorFocus"
  },
  {
    "key": "cmd+shift+f",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "!editorFocus"
  },
  {
    "key": "cmd+0",
    "command": "workbench.action.terminal.focus",
    "when": "!terminalFocus"
  }, {
    "key": "cmd+0",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+b",
    "command": "editor.action.goToDeclaration",
    "when": "editorHasDefinitionProvider && editorTextFocus"
  },
  {
    "key": "shift+cmd+backspace",
    "command": "editsHistory.moveCursorToPreviousEdit"
  },
  {
    "key": "cmd+j",
    "command": "-editsHistory.moveCursorToPreviousEdit"
  }
]